<Window x:Class="Chat1._0.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chat1._0"
        mc:Ignorable="d"
        Title="Login" Height="200" Width="200" WindowStartupLocation="CenterScreen" WindowStyle="None" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="btnUsuario" Content="Ingresar" HorizontalAlignment="Left" Margin="60,145,0,0" VerticalAlignment="Top" Width="95" Click="btnUsuario_Click"/>
        <TextBox x:Name="txtUsuario" HorizontalAlignment="Left" Height="23" Margin="60,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Usuario: " HorizontalAlignment="Left" Margin="21,56,0,0" VerticalAlignment="Top" Width="57"/>

    </Grid>
</Window>
